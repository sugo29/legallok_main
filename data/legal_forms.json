{
  "forms": [
    {
      "id": "rti",
      "title": "Right to Information (RTI) Application",
      "description": "Apply for information under the Right to Information Act, 2005",
      "icon": "fa-file-alt",
      "category": "Government",
      "fields": [
        {
          "name": "applicantName",
          "label": "Full Name of Applicant",
          "type": "text",
          "required": true
        },
        {
          "name": "address",
          "label": "Complete Address",
          "type": "textarea",
          "required": true
        },
        {
          "name": "contactNumber",
          "label": "Mobile Number",
          "type": "tel",
          "required": true
        },
        {
          "name": "email",
          "label": "Email ID",
          "type": "email",
          "required": false
        },
        {
          "name": "authorityName",
          "label": "Name of Public Authority",
          "type": "text",
          "required": true
        },
        {
          "name": "authorityAddress",
          "label": "Address of Public Authority",
          "type": "textarea",
          "required": true
        },
        {
          "name": "informationSought",
          "label": "Information Sought (Point-wise)",
          "type": "textarea",
          "required": true,
          "placeholder": "1. \n2. \n3. "
        },
        {
          "name": "paymentMode",
          "label": "Mode of Payment",
          "type": "select",
          "required": true,
          "options": [
            {"value": "ipo", "text": "Indian Postal Order"},
            {"value": "dd", "text": "Demand Draft"},
            {"value": "cash", "text": "Cash (if submitting in person)"},
            {"value": "online", "text": "Online Payment"}
          ]
        },
        {
          "name": "paymentDetails",
          "label": "Payment Details (Number/Reference)",
          "type": "text",
          "required": true
        },
        {
          "name": "place",
          "label": "Place",
          "type": "text",
          "required": false
        },
        {
          "name": "date",
          "label": "Date",
          "type": "date",
          "required": true
        }
      ],
      "instructions": [
        {
          "title": "Step 1: Applicant Details",
          "content": "Fill in your complete name, address, and contact information. Ensure your mobile number and email are correct for communication."
        },
        {
          "title": "Step 2: Public Authority",
          "content": "Mention the complete name and address of the government department or public authority you're addressing the RTI to."
        },
        {
          "title": "Step 3: Information Sought",
          "content": "Clearly specify the information you require. Be precise and mention relevant time periods if applicable. You can ask multiple questions but keep them numbered."
        },
        {
          "title": "Step 4: Payment Details",
          "content": "RTI applications typically require a fee of â‚¹10. Mention how you've paid (IPO, demand draft, court fee stamp, etc.) and include proof."
        },
        {
          "title": "Step 5: Submit Application",
          "content": "Sign and date the application. Keep a copy for your records before submitting to the Public Information Officer of the concerned department."
        }
      ]
    },
